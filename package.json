{
  "name": "travel-booking-app",
  "version": "1.0.0",
  "private": true,
  "description": "A React-based AI-powered Travel Booking Web App with search, package listings, and booking flow.",
  "author": "Your Name",
  "license": "MIT",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/travel-booking-app"
  },
  "bugs": {
    "url": "https://github.com/your-username/travel-booking-app/issues"
  },
  "keywords": [
    "travel",
    "booking",
    "react",
    "ai",
    "framer-motion",
    "react-router",
    "axios",
    "frontend"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx || true",
    "lint:fix": "eslint src --ext .js,.jsx --fix || true",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.2",
    "framer-motion": "^12.23.22"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": "warn",
      "no-empty": "off",
      "react/no-unescaped-entities": "off",
      "import/first": "off",
      "jsx-a11y/role-supports-aria-props": "off"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
